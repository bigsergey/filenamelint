{
  "name": "filenamelint",
  "version": "0.1.0",
  "description": "Linter for file and directory names",
  "engines": {
    "node": ">=12.10.0",
    "npm": ">=6.10.3"
  },
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run build:live",
    "build": "tsc -p .",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "lint": "eslint \"src/**\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bigsergey/filenamelint.git"
  },
  "keywords": [
    "linter",
    "filename",
    "directory",
    "name"
  ],
  "author": "bigsergey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bigsergey/filenamelint/issues"
  },
  "homepage": "https://github.com/bigsergey/filenamelint#readme",
  "dependencies": {
    "fast-glob": "3.1.0",
    "function-pipe": "1.2.4",
    "kebab-case": "1.0.0"
  },
  "devDependencies": {
    "@types/node": "12.12.7",
    "@typescript-eslint/eslint-plugin": "2.7.0",
    "@typescript-eslint/parser": "2.7.0",
    "eslint": "6.6.0",
    "nodemon": "1.19.4",
    "ts-node": "8.5.0",
    "typescript": "3.7.2"
  }
}
