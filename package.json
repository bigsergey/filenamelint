{
  "name": "filenamelint",
  "version": "0.3.1",
  "description": "Linter for file and directory names",
  "engines": {
    "node": ">=11.14.0",
    "npm": ">=6.10.3"
  },
  "bin": {
    "filenamelint": "./lib/cli/index.js"
  },
  "main": "lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "start": "npm run build:live",
    "build": "tsc -p .",
    "build:live": "tsc -w",
    "lint": "eslint \"src/**\"",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bigsergey/filenamelint.git"
  },
  "keywords": [
    "linter",
    "filename",
    "directory",
    "name"
  ],
  "author": "bigsergey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bigsergey/filenamelint/issues"
  },
  "homepage": "https://github.com/bigsergey/filenamelint#readme",
  "dependencies": {
    "commander": "4.0.1",
    "fast-glob": "3.1.1",
    "kebab-case": "1.0.0"
  },
  "devDependencies": {
    "@types/jest": "24.0.23",
    "@types/node": "12.12.16",
    "@typescript-eslint/eslint-plugin": "2.10.0",
    "@typescript-eslint/parser": "2.10.0",
    "eslint": "6.7.2",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-prettier": "3.1.1",
    "husky": "3.1.0",
    "jest": "24.9.0",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1",
    "ts-jest": "24.2.0",
    "ts-node": "8.5.4",
    "typescript": "3.7.3"
  }
}
