{
  "name": "filenamelint",
  "version": "0.6.0",
  "description": "Linter for file and directory names",
  "engines": {
    "node": ">=11.14.0",
    "npm": ">=6.10.3"
  },
  "bin": {
    "filenamelint": "./lib/cli/index.js"
  },
  "main": "lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "start": "tsc -w",
    "build": "tsc -p .",
    "lint": "eslint \"src/**\" && markdownlint '**/*.md' --ignore node_modules",
    "test": "npm audit && jest --coverage",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "*.ts": "eslint --fix",
    "*.md": "markdownlint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bigsergey/filenamelint.git"
  },
  "keywords": [
    "linter",
    "filename",
    "directory",
    "name"
  ],
  "author": "bigsergey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bigsergey/filenamelint/issues"
  },
  "homepage": "https://github.com/bigsergey/filenamelint#readme",
  "dependencies": {
    "camelcase": "6.2.0",
    "chalk": "4.1.0",
    "commander": "6.2.0",
    "fast-glob": "3.2.4",
    "kebab-case": "1.0.0",
    "snake-case": "3.0.4"
  },
  "devDependencies": {
    "@types/jest": "26.0.15",
    "@types/node": "14.14.5",
    "@typescript-eslint/eslint-plugin": "4.6.0",
    "@typescript-eslint/parser": "4.6.0",
    "eslint": "7.12.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.3.0",
    "jest": "26.6.1",
    "lint-staged": "10.5.0",
    "markdownlint-cli": "0.24.0",
    "prettier": "2.1.2",
    "ts-jest": "26.4.3",
    "ts-node": "9.0.0",
    "typescript": "4.0.5"
  }
}
