{
  "name": "filenamelint",
  "version": "0.5.0",
  "description": "Linter for file and directory names",
  "engines": {
    "node": ">=11.14.0",
    "npm": ">=6.10.3"
  },
  "bin": {
    "filenamelint": "./lib/cli/index.js"
  },
  "main": "lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "start": "tsc -w",
    "build": "tsc -p .",
    "lint": "eslint \"src/**\" && markdownlint '**/*.md' --ignore node_modules",
    "test": "npm audit && jest --coverage",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "*.ts": "eslint --fix",
    "*.md": "markdownlint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bigsergey/filenamelint.git"
  },
  "keywords": [
    "linter",
    "filename",
    "directory",
    "name"
  ],
  "author": "bigsergey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bigsergey/filenamelint/issues"
  },
  "homepage": "https://github.com/bigsergey/filenamelint#readme",
  "dependencies": {
    "camelcase": "5.3.1",
    "commander": "4.1.0",
    "fast-glob": "3.1.1",
    "kebab-case": "1.0.0",
    "snake-case": "3.0.3"
  },
  "devDependencies": {
    "@types/jest": "24.9.1",
    "@types/node": "13.5.0",
    "@typescript-eslint/eslint-plugin": "2.17.0",
    "@typescript-eslint/parser": "2.17.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "4.2.1",
    "jest": "25.1.0",
    "lint-staged": "10.0.2",
    "markdownlint-cli": "0.21.0",
    "prettier": "1.19.1",
    "ts-jest": "25.0.0",
    "ts-node": "8.6.2",
    "typescript": "3.7.5"
  }
}
