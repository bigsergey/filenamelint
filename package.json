{
  "name": "filenamelint",
  "version": "0.6.0",
  "description": "Linter for file and directory names",
  "engines": {
    "node": ">=11.14.0",
    "npm": ">=6.10.3"
  },
  "bin": {
    "filenamelint": "./lib/cli/index.js"
  },
  "main": "lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "start": "tsc -w",
    "build": "tsc -p .",
    "lint": "eslint \"src/**\" && markdownlint '**/*.md' --ignore node_modules",
    "test": "npm audit && jest --coverage",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "*.ts": "eslint --fix",
    "*.md": "markdownlint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bigsergey/filenamelint.git"
  },
  "keywords": [
    "linter",
    "filename",
    "directory",
    "name"
  ],
  "author": "bigsergey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bigsergey/filenamelint/issues"
  },
  "homepage": "https://github.com/bigsergey/filenamelint#readme",
  "dependencies": {
    "camelcase": "6.0.0",
    "chalk": "4.0.0",
    "commander": "5.1.0",
    "fast-glob": "3.2.2",
    "kebab-case": "1.0.0",
    "snake-case": "3.0.3"
  },
  "devDependencies": {
    "@types/jest": "25.2.1",
    "@types/node": "13.13.4",
    "@typescript-eslint/eslint-plugin": "2.29.0",
    "@typescript-eslint/parser": "2.29.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "husky": "4.2.5",
    "jest": "25.4.0",
    "lint-staged": "10.1.7",
    "markdownlint-cli": "0.22.0",
    "prettier": "2.0.5",
    "ts-jest": "25.4.0",
    "ts-node": "8.9.1",
    "typescript": "3.8.3"
  }
}
